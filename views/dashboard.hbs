        {{#if blogs}}
          <div class="col-lg-9 col-md-12">
            <div class="panel panel-default">
              <div class="panel-heading">
                <h2><i class="fa fa-flag-o red"></i><strong>Public Blogs</strong></h2>
              </div>
                {{!--  --}}

                <table class="table bootstrap-datatable personal-task">
                  <thead>
                    <tr>
                      <th></th>
                     
                      <th>Date</th>
                       <th>Title</th>
                      <th>Category</th>
                      <th>Tags</th>
                       <th>Status</th>
                      <th></th>
                    </tr>
                  </thead>
                  <tbody>
                    {{#each blogs}}
                    <tr>
                      <td></td>
                      <td>{{formateDate createdAt 'MMMM Do YYYY, h:mm:ss a'}}</td>
                      <td><a href="/blogs/{{_id}}">{{title}}</a></td>
                      <td><span class="badge badge-primary">{{category}}</span></td>
                      <td><span class="badge badge-primary">{{tags}}</span></td>
                      <td><span class="badge badge-primary">{{status}}</span></td>
                      <td></td>
                    </tr>
                    {{/each}}
                  </tbody>
                </table>
            </div>
          </div>
        {{else}}
          <p>You have no blogs to view yet</p>
        {{/if}}
